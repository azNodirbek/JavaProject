<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.shape.SVGPath?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.Label?>
<GridPane fx:controller="main.Adding" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="50.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <VBox alignment="CENTER" spacing="20.0" GridPane.rowIndex="1">
        <children>
            <Label fx:id="welcomeMessage" text="Enter Librarian" textFill="#aaaaaa">
                <font>
                    <Font name="SansSerif Bold" size="24.0"/>
                </font>
            </Label>
            <TextField fx:id="Lib_Username" promptText="Login">
                <font>
                    <Font name="SansSerif Regular" size="16.0"/>
                </font>
                <padding>
                    <Insets bottom="7.0" left="15.0" right="15.0" top="7.0"/>
                </padding>
            </TextField>
            <PasswordField fx:id="Lib_password" promptText="Password">
                <font>
                    <Font name="SansSerif Regular" size="16.0"/>
                </font>
                <padding>
                    <Insets bottom="7.0" left="15.0" right="15.0" top="7.0"/>
                </padding>
            </PasswordField>
            <PasswordField fx:id="Lib_confirm" promptText="Password">
                <font>
                    <Font name="SansSerif Regular" size="16.0"/>
                </font>
                <padding>
                    <Insets bottom="7.0" left="15.0" right="15.0" top="7.0"/>
                </padding>
            </PasswordField>
            <Button fx:id="Librarian_adding" onAction="#lib_register" mnemonicParsing="false"
                    text="Reg" textFill="WHITE">
                <padding>
                    <Insets bottom="7.0" top="7.0"/>
                </padding>
                <font>
                    <Font name="SansSerif Bold" size="16.0"/>
                </font>
            </Button>
        </children>
    </VBox>
</GridPane>